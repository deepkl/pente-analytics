<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
        <!ENTITY visallo "http://visallo.org#" >
        <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
        <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
        <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
        <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
        ]>


<rdf:RDF xmlns="http://visallo.org/game#"
         xml:base="http://visallo.org/game"
         xmlns:visallo="http://visallo.org#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://visallo.org/game">
        <owl:imports rdf:resource="http://visallo.org"/>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <!-- http://visallo.org/game#hasEntity -->

    <owl:ObjectProperty rdf:about="http://visallo.org/game#hasEntity">
        <rdfs:label>Has Entity</rdfs:label>
        <visallo:intent>artifactHasEntity</visallo:intent>
        <rdfs:domain rdf:resource="http://visallo.org/game#document"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>



    <!-- http://visallo.org/game#entityHasImage -->

    <owl:ObjectProperty rdf:about="http://visallo.org/game#entityHasImage">
        <rdfs:label xml:lang="en">Has Image</rdfs:label>
        <visallo:intent>entityHasImage</visallo:intent>
        <rdfs:range rdf:resource="http://visallo.org/game#raw"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>

    
    
    <!-- http://visallo.org/game#rawContainsImageOfEntity -->

    <owl:ObjectProperty rdf:about="http://visallo.org/game#rawContainsImageOfEntity">
        <rdfs:label xml:lang="en">Contains Image of</rdfs:label>
        <visallo:intent>artifactContainsImageOfEntity</visallo:intent>
        <visallo:intent>artifactContainsImage</visallo:intent>
        <rdfs:domain rdf:resource="http://visallo.org/game#raw"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>

    
    
    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <!-- http://visallo.org/game#fileSize -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/game#fileSize">
        <rdfs:label>File Size</rdfs:label>
        <visallo:intent>media.fileSize</visallo:intent>
        <rdfs:domain rdf:resource="http://visallo.org/game#raw"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>


    
    <!-- http://visallo.org/game#yAxisFlipped -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/game#yAxisFlipped">
        <rdfs:label>Y Axis Flipped</rdfs:label>
        <visallo:userVisible rdf:datatype="&xsd;boolean">false</visallo:userVisible>
        <visallo:intent>media.yAxisFlipped</visallo:intent>
        <rdfs:domain rdf:resource="http://visallo.org/game#raw"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>


    
    <!-- http://visallo.org/game#clockwiseRotation -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/game#clockwiseRotation">
        <rdfs:label>Clockwise Rotation</rdfs:label>
        <visallo:userVisible rdf:datatype="&xsd;boolean">false</visallo:userVisible>
        <visallo:intent>media.clockwiseRotation</visallo:intent>
        <rdfs:domain rdf:resource="http://visallo.org/game#raw"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    
    
    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <!-- http://visallo.org/game#image -->

    <owl:Class rdf:about="http://visallo.org/game#image">
        <rdfs:label xml:lang="en">Image</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://visallo.org/game#raw"/>
        <visallo:intent>entityImage</visallo:intent>
        <visallo:intent>image</visallo:intent>
        <visallo:displayType xml:lang="en">image</visallo:displayType>
        <visallo:color xml:lang="en">rgb(176, 87, 53)</visallo:color>
    </owl:Class>



    <!-- http://visallo.org/game#document -->

    <owl:Class rdf:about="http://visallo.org/game#document">
        <rdfs:label>Document</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://visallo.org/game#raw"/>
        <visallo:intent>document</visallo:intent>
        <visallo:displayType>document</visallo:displayType>
        <visallo:color>rgb(28, 137, 28)</visallo:color>
    </owl:Class>



    <!-- http://visallo.org/game#raw -->

    <owl:Class rdf:about="http://visallo.org/game#raw">
        <rdfs:label>Raw</rdfs:label>
        <visallo:glyphIconFileName>raw.png</visallo:glyphIconFileName>
        <visallo:color>rgb(28, 137, 28)</visallo:color>
    </owl:Class>


</rdf:RDF>
